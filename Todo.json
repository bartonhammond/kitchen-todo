							"components": [
								{
									"name": "filterOptions",
									"custom_template": "",
									"html": "\n<template name=\"filterOptions\">\n\t<div class=\"well\">\n\t\t<div style=\"width: 200px; float: left;\">\n           <span id=\"todo-count\">\n             <strong>\n               {{todos_not_completed}}\n             </strong>\n             {{#if todos_one_not_completed}}item{{else}}items{{/if}} left\n           </span>\n\t\t</div>\n      \n\t\t<div style=\"text-align: right; float: right; width: 200px;\">\n          {{#if todos_completed}}\n          <a href=\"clear_completed\">\n            Clear completed\n            </a>\n          {{/if}}\n        </div>\n      \n        <div style=\"text-align: center; margin:0 auto;\">\t    \n          <a href=\"all\">All&nbsp;</a>\n          <a href=\"active\">Active&nbsp;</a>\n          <a href=\"completed\">Completed</a>\n        </div>\n\n\t</div>\n</template>\n",
									"": "_",
									"js": "Template.filterOptions.events({\n  \"click [href=clear_completed]\" : function(e) {\n    e.preventDefault();\n    Meteor.call('clearCompleted');\n  },\n  \"click [href=all]\": function(e) {\n    e.preventDefault();\n    pageSession.set(\"TodosViewSearchString\",\"\");\n  },\n  \"click [href=active]\": function(e) {\n    e.preventDefault();\n    pageSession.set(\"TodosViewSearchString\",\"false\");\n  },\n  \"click [href=completed]\": function(e) {\n    e.preventDefault();\n    pageSession.set(\"TodosViewSearchString\",\"true\");\n  }\n});\n\nTemplate.filterOptions.helpers({\n  todos_not_completed: function() {\n    return Todos.find({completed: false}).count();\n  },\n  todos_completed: function() {\n    return Todos.find({completed: true}).count();\n  },\n  todos_one_not_completed: function() {\n    return Todos.find({completed: false}).count() == 1;\n  }\n});\n",
